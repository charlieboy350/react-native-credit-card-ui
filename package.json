{
  "name": "react-native-credit-card-ui-pkg",
  "version": "1.1.3",
  "description": "Credit card UI module",
  "main": "./src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace custom-credit-card",
    "test": "jest --verbose",
    "typecheck": "tsc --noEmit",
    "lint": "yarn run eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "credit-card-ui",
    "braintree",
    "paypal",
    "credit-card",
    "credit-card",
    "accept-blue",
    "payment-gateway-UI",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlieboy350/react-native-credit-card-ui.git"
  },
  "author": "Naeem ur Rehman <naeem.ur.rehman.co@gmail.com> (https://github.com/charlieboy350)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlieboy350/react-native-credit-card-ui/issues"
  },
  "homepage": "https://github.com/charlieboy350/react-native-credit-card-ui#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@commitlint/config-conventional": "^19.4.1",
    "@evilmartians/lefthook": "^1.7.14",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "^0.76.5",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.4",
    "@types/react-native": "0.72.8",
    "@types/react-test-renderer": "^19.0.0",
    "babel-jest": "^29.7.0",
    "commitlint": "^19.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.4.2",
    "react": "18.3.1",
    "react-native": "0.75.2",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "^19.0.0",
    "release-it": "^17.6.0",
    "typescript": "^5.5.4"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-svg": "^15.8.0"
  },
  "dependencies": {
    "copyfiles": "^2.4.1",
    "credit-card-type": "^10.0.1",
    "creditcardutils": "^1.0.0",
    "globals": "^15.14.0",
    "react-native-svg": "^15.8.0"
  },
  "workspaces": [
    "customCreditCard"
  ],
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": ">= 18.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/customCreditCard/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "directories": {
    "lib": "lib"
  }
}
